{
  "name": "vue-to-excel",
  "version": "0.1.0",
  "main": "dist/vue-to-excel.umd.js",
  "module": "dist/vue-to-excel.esm.js",
  "unpkg": "dist/vue-to-excel.min.js",
  "browser": {
    "./sfc": "src/vue-to-excel.vue"
  },
  "scripts": {
    "build": "npm run build:umd & npm run build:es & npm run build:unpkg",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/vue-to-excel.umd.js",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/vue-to-excel.esm.js",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/vue-to-excel.min.js"
  },
  "devDependencies": {
    "@rollup/plugin-buble": "^0.21.3",
    "@rollup/plugin-commonjs": "^11.1.0",
    "eslint": "^8.26.0",
    "eslint-plugin-vue": "^7.0.0-beta.4",
    "rollup": "^1.17.0",
    "rollup-plugin-vue": "^5.0.1",
    "vue": "^2.6.10",
    "vue-template-compiler": "^2.6.10"
  }
}
